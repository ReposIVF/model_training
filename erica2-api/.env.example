# ============================================
# ERICA API - Environment Configuration Example
# ============================================
# Copy this file to .env.development, .env.staging, or .env.production
# and fill in the appropriate values.
# 
# Domain: https://erica.ivf20.app
# ============================================

# Environment: development | staging | production
ERICA_ENV=development
ERICA_VERSION=2.0.0

# Server Configuration
HOST=0.0.0.0
PORT=8001
# API_URL is the public URL where the API is accessible
API_URL=http://localhost:8001

# Security
API_SECRET_KEY=ed1c6740-d82a-4359-9395-b2b5539f7be1
VALIDATION_PASS_KEY=680bb7cc-0f5c-4e4c-bae2-2b74ce62621e

# Parse Server Configuration
PARSE_SERVER_URL=https://dish-s.ivf20.app/db
PARSE_APPLICATION_ID=your_parse_app_id
PARSE_REST_API_KEY=your_parse_rest_api_key

# AWS S3 Configuration
ERICA_S3_BUCKET=your_bucket_name
ERICA_S3_REGION=us-east-1
ERICA_S3_ACCESS_KEY=your_aws_access_key
ERICA_S3_SECRET_KEY=your_aws_secret_key

# Model Configuration (relative to models/ folder)
MODEL_SCORING=erica_model2.pth
MODEL_CROPPER=erica_cropper.pt
MODEL_SEGMENTOR=erica_segmentor_n.pt
MODEL_SCALER=scaler_info.json

# Logging
LOG_LEVEL=DEBUG
